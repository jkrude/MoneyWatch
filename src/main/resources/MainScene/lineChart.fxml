<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jkrude.controller.LineChartController">
   <children>
     <BorderPane prefHeight="720.0" prefWidth="1280.0">
       <center>
         <LineChart fx:id="lineChart" minHeight="-Infinity" minWidth="-Infinity"
           BorderPane.alignment="CENTER">
           <xAxis>
             <NumberAxis side="BOTTOM"/>
           </xAxis>
           <yAxis>
             <NumberAxis side="LEFT"/>
           </yAxis>
           <BorderPane.margin>
             <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
           </BorderPane.margin>
         </LineChart>
        </center>
       <top>
         <ButtonBar BorderPane.alignment="TOP_LEFT">
           <buttons>
             <Button contentDisplay="CENTER" mnemonicParsing="false" text="Data selection"/>
             <Button fx:id="backButton" mnemonicParsing="false" text="Back"
               ButtonBar.buttonData="LEFT"/>
           </buttons>
           <BorderPane.margin>
             <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
           </BorderPane.margin>
         </ButtonBar>
       </top>
      </BorderPane>
   </children>
</AnchorPane>
