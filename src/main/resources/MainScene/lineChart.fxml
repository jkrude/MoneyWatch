<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jkrude.controller.LineChartController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
        <center>
             <LineChart fx:id="lineChart" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720" prefWidth="1280" BorderPane.alignment="CENTER">
               <xAxis>
                   <NumberAxis side="BOTTOM" />
               </xAxis>
               <yAxis>
                   <NumberAxis side="LEFT" />
               </yAxis>
             </LineChart>
        </center>
        <top>
            <Button fx:id="backButton" mnemonicParsing="false" text="Back" BorderPane.alignment="TOP_LEFT" />
        </top>
      </BorderPane>
   </children>
</AnchorPane>
